    "start": "node app.js |  webpack --watch |  node tool/watch.js",
    "migrate": "knex migrate:latest",
    "build": "cnpm install && webpack && node tool/build",
    "deploy": "git branch -D tmp && git fetch origin develop:tmp && git merge tmp && npm run build  &&  npm run cdn && pm2 restart app.js",
    "push": "pbranch=`git branch | grep \\* | cut -c3-` && git branch -D develop && git fetch origin develop:develop && git merge develop && git checkout develop && git merge $pbranch && git push origin develop && git checkout $pbranch",
    "cdn": "alisdk tmp/assets/javascripts/controller,tmp/assets/javascripts/production.js"
  },
